    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "2.3.1",
    "react-scripts": "4.0.3",
    "shx": "0.3.3",
    "typescript": "4.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/setupTests.{ts,tsx}",
      "!src/serviceWorker.{ts,jsx}",
      "!src/**/*.(spec|test|stories).{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "coverageReporters": [
      "cobertura",
      "text",
      "html"
    ]
  }
}
